  [DOat1_11] !at1_11 -> (at1_11'=true);
  [DOat1_12] !at1_12 -> (at1_12'=true);
  [getTime] at1_1 & at1_2 & at1_3 & at1_4 & at1_5 & at1_6 & at1_7 & at1_8 & at1_9 & at1_10 & at1_11 & at1_12 & !end->(end'=true);
endmodule

module r1
  r1Time:[0..TT] init 0;
  r1Loc:[0..12] init 0;
  r1Fail:[0..1] init 0;
  [DOat1_1]  r1TimeOk_at1_1 -> r1at1_1prob:(r1Time'=r1Time+r1at1_1Time+r1travelTOat1_1) & (r1Loc'=1)+ (1-r1at1_1prob):(r1Time'=r1Time+r1at1_1Time+r1travelTOat1_1) & (r1Loc'=1)& (r1Fail'=1);
