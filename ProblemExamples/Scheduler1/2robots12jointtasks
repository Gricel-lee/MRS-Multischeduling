module scheduler
  at1_1:bool;
  at1_2:bool;
  at1_3:bool;
  at1_4:bool;
  at1_5:bool;
  at1_6:bool;
  at1_7:bool;
  at1_8:bool;
  at1_9:bool;
  at1_10:bool;
  at1_11:bool;
  at1_12:bool;
  end:bool;//endmission
  [DOat1_1] !at1_1 -> (at1_1'=true);
  [DOat1_2] !at1_2 -> (at1_2'=true);
  [DOat1_3] !at1_3 -> (at1_3'=true);
  [DOat1_4] !at1_4 -> (at1_4'=true);
  [DOat1_5] !at1_5 -> (at1_5'=true);
  [DOat1_6] !at1_6 -> (at1_6'=true);
